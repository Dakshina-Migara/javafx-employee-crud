<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.practice.controller.SaveEmployeeController">
    <children>
        <TextField fx:id="txtName" layoutX="129.0" layoutY="94.0" prefHeight="25.0" prefWidth="338.0"
                   promptText="name"/>
        <TextField fx:id="txtNic" layoutX="129.0" layoutY="147.0" prefHeight="25.0" prefWidth="338.0" promptText="nic"/>
        <TextField fx:id="txtAge" layoutX="129.0" layoutY="200.0" prefHeight="25.0" prefWidth="338.0" promptText="age"/>
        <TextField fx:id="txtSalary" layoutX="131.0" layoutY="250.0" prefHeight="25.0" prefWidth="338.0"
                   promptText="salary"/>
        <Button layoutX="261.0" layoutY="321.0" mnemonicParsing="false" onAction="#save" text="save"/>
        <Button layoutX="14.0" layoutY="22.0" mnemonicParsing="false" text="back"/>
    </children>
</AnchorPane>
